{
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 110,
  "useTabs": false,
  "semi": true,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    },
    {
      "files": "*.component.html",
      "options": {
        "parser": "angular"
      }
    }
  ]
}
